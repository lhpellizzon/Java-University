/*
 
 * ---- Operators ---- *

() n inside brackets are evaluated first ;
* / and % highest precedence and go left to right ; 
+ and - lowest precedence and go left to right ; 

*/

public class Operators {

	public static void main(String[] args) {
		
		float div = (float) (2/3.0);
		
		System.out.println(div);

	}

}
