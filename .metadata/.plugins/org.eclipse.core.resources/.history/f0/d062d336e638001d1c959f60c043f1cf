package OutputHelloWorld;

public class UnicodeTraining {

	public static void main(String[] args) {
		System.out.println("\u00A9 2022\n");
		System.out.println("The circumference of a circle is 2\u03C0r\n");
		System.out.println("For = f\u00FCr in German.");
		System.out.println("For = f\u00AE in German.");
	}

}
